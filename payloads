#!/bin/bash
tput setaf 7 ; tput setab 4 ; tput bold ; printf '%30s%s%-10s\n' "Payloads disponíveis e funcionando" ; tput sgr0 ; echo ""
tput setaf 1 ; tput bold ; printf '%s' "Exemplos FUNCIONAIS de payloads " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 4 ; echo "Todas funcionam, só copiar e usar, caso alguma dê problema, veja se o 'host' dela está adicionada no arquivos de hosts, comando 'addhost' e a adicione" ; echo ""
tput setaf 1 ; tput bold ; printf '%s' "VIVO " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 3 ; echo "CONNECT [host_port]@interatividade.vivo.ddivulga.com/produto [protocol][crlf]Host: interatividade.vivo.ddivulga.com/produto[crlf]X-Forwarded-For: interatividade.vivo.ddivulga.com/produto[crlf][crlf]" ; echo ""
tput setaf 1 ; tput bold ; printf '%s' "TIM " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 3 ; echo "CONNECT [host_port] HTTP/1.1[crlf][crlf]GET http://pos.tim.com.br/ [protocol][crlf]Host: pos.tim.com.br[crlf]@NetGratisMobile [crlf][crlf]" ; echo ""
tput setaf 1 ; tput bold ; printf '%s' "Oi " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 3 ; echo "CONNECT [host_port]@www.rdio.com/rewrite.pandora.com [protocol][crlf][split]GET http://www.rdio.com/rewrite.pandora.com/ HTTP/1.1[crlf]Host: www.rdio.com/rewrite.pandora.com[crlf]X-Online-Host: www.rdio.com/rewrite.pandora.com[crlf]X-Forward-Host: www.rdio.com/rewrite.pandora.com[crlf]X-Forwarded-For: www.rdio.com/rewrite.pandora.com[crlf]Connection: Keep-Alive[crlf]User-Agent: [ua][crlf][crlf]" ; echo ""
tput setaf 1 ; tput bold ; printf '%s' "CLARO " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 3 ; echo "GET http://www.clarosync.com HTTP/1.1[lf][method][crlf]Proxy-Authorization: Basic:  http://www.clarosync.com [lf][split][protocol][ua][port][auth][lf][lf][netdata][lf] [lf][lf]" ; echo ""
tput setaf 3 ; tput bold ; printf '%s' "Se algum HOST cair " ; tput setaf 7 ; printf '%s' "- " ; tput setaf 2 ; echo "Apenas troque o HOST da payload pelo host atual, use a mesma payload" ; echo ""
tput sgr0
